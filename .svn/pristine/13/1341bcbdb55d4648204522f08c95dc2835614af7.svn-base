/* 编辑/保存/取消按钮 */
.cancel,
.bjimg,
.upico,
.saveico,
.delico {
	height: 20px;
	float: right;
	padding-left: 20px;
	line-height: 21px;
	font-size: 14px;
	margin-left: 10px;
	cursor: pointer;
	filter: grayscale(100%);
	color: #7F8493;

	&:after {
		display: inline-block;
		vertical-align: middle;
	}
}

.cancel {
	background: url(../../common/icons/examine/icon_close.png)no-repeat left center;
	background-size: auto 100%;
}

.cancel:after {
	content: '取消';
	font-weight: 400;
}

.saveico {
	background: url(../../common/icons/examine/icon_preservation.png)no-repeat left center;
	background-size: auto 100%;
	filter: grayscale(0);
	color: #008AFF;
}

.saveico:after {
	content: '保存';
	font-weight: 400;
}

.bjimg {
	background: url(../../common/icons/examine/icon_edit.png)no-repeat left center;
	background-size: auto 100%;
}

.bjimg:after {
	content: '编辑';
	font-weight: 400;
}

.upico {
	background: url(../../common/icons/icon_Preservation.png)no-repeat left center;
	background-size: auto 100%;
}

.upico:after {
	content: '上传附件';
	font-weight: 400;
}

.delico {
	background: url(../../common/icons/icon_delete.png)no-repeat left center;
	background-size: auto 100%;
}

.delico:after {
	content: '删除';
	font-weight: 400;
}
