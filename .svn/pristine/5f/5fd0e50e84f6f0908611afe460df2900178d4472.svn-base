    },
    mounted() {
      // this.gosetDefault()
    },
    methods: {
      gosetDefault() {
        let n = cystyle
        let s = cydata
        let that = this
        let cy =  that.$cytoscape({
          container: document.getElementById('cy'),
          layout: {
            name: 'cose',
            idealEdgeLength: 100,  // 莫名其妙的变大框
            nodeOverlap: 20,
            refresh: 20,
            fit: true, // 水平垂直居中
            padding: 30,
            randomize: false, // 暂时没发现用处
            componentSpacing: 100,
            nodeRepulsion: 400000, // 大小间距比例啥的
            edgeElasticity: 100, // 暂时没发现用处
            nestingFactor: 5, // 暂时没发现用处
            gravity: 80,
            numIter: 1000, // 这个一定要，随便设置，不给的话，各个圆会叠一块儿
            initialTemp: 200, // 暂时没发现用处
            coolingFactor: 0.9, // 散列点位置，值越低越贴近圆边
            minTemp: 1.0 // 没啥用
          },
          zoom: 1,
          minZoom: 1e-50,
          maxZoom: 1e50,
          zoomingEnabled: false,//定义是否可缩放
          style: n,
          elements: s
        })
        return false
      },
      drawCloud(){
        let that = this
        // alert('eeeeee')
        let charObj = document.getElementById('wordCloud')
        charObj.style.width = '600px';
        let wordCloud = this.$echarts.init(charObj);
        // wordCloud.resize()
        // wordCloud.style.width = 500 + 'px';
        function createRandomItemStyle() {
            return {
                normal: {
                    color: 'rgb(' + [
                        Math.round(Math.random() * 160),
                        Math.round(Math.random() * 160),
                        Math.round(Math.random() * 160)
                    ].join(',') + ')'
                }
            };
        }
        let text = ['Sam S Club','Macys','Amy Schumer','Jurassic World','Charter Communications','Chick Fil A','Planet Fitness','Pitch Perfect','Express','Home','Johnny Depp','Lena Dunham','Lewis Hamilton','KXAN','Mary Ellen Mark','Farrah Abraham','Rita Ora','Serena Williams','NCAA baseball tournament','Point Break']
        let data = [];
        for(var i = 0; i < text.length; i++){
            let obj = {};
            obj.name = text[i];
            obj.value = Math.random()*10000;
            data.push(obj)
        }
        let option = {
            title: {
                text: null,
                link: ''
            },
            tooltip: {
                show: false, // 悬停提示
            },
            series: [{
                name: 'Google Trends',
                type: 'wordCloud',
                size: ['80%', '80%'],
                textRotation : [0, 45, 90, -45],
                textPadding: 0,
                autoSize: {
                    enable: true,
                    minSize: 14,
                    maxSize: 20
                },
                 textStyle: {//文字样式设置
                    normal: {
                        color: function() {//颜色
                            return 'rgb(' + [//返回随机生成的颜色
                                Math.round(Math.random() * 160),
                                Math.round(Math.random() * 160),
                                Math.round(Math.random() * 160)
                            ].join(',') + ')';
                        }
                    },
                    emphasis: {//鼠标划入样式
                        shadowBlur: 10,//文字阴影模糊度
                        shadowColor: '#333'//文字阴影颜色
                    }
                },
                data:data
            }]
        }
        wordCloud.setOption(option)
      },
      handleCurrentChange (val) {
        this.$emit('handleCurrentChange', val)
      },
      handleSizeChange (val) {
        this.$emit('handleSizeChange', val)
      },
      sWindow(type, keyVal) {
        // 查询的类型   基因、疾病、药物    keyVal 是查询参数
        let that = this
        that.$emit('sWindow')
      },
      changeTab(item, i) {
        // 改变选中的选项
        let that = this
        that.$emit('changeTab', item, i)
      },
      pushHead(type, scope) {
        let that = this
        that.$emit('pushHead', type, scope)
      },
      cmmk(i) {
        let that = this
        that.$emit('cmmk', i)
      },
      closePanel(i) {
        let that = this
        that.$emit('closePanel', i)
      },
      blockSearch(formInlines) { // 查询
        let that = this
        that.$emit('blockSearch', formInlines)
       },
      changeValidata(value) {
        // 更改标记的提示
        let that = this
        that.scope1.hasValidata = value == '0' ? '1' : '0'
        let url = 'report/variant/modifyReportVariantValidate'
        axios.post(URL.api_name + url, {
          code: that.scope1.code,
          hasValidata: that.scope1.hasValidata
        }).then(function(res) {
          if(res.data.code == '100') {
            that.$message({
              type: 'success',
              message: res.data.message,
              duration: 1000
            })
            that.blockSearch(that.formInline)
          } else {
            // 失败,重新查询
            that.$message({
              type: 'error',
              message: res.data.message,
              duration: 1000
            })
          }
        }).catch(function(res){
          // 接口报错了
        })
      }
    },
    components: {
      Pager: Pager
    },
    watch: {
      formDatas: function(newVal,oldVal){
        let that = this
        this.formData = newVal
        this.formData1 = JSON.parse(JSON.stringify(this.formData))
        for (let i in that.formData1.list) {
          // that.formData1.list[i]['isEdit'] = false
          that.$set(that.formData1.list[i], 'isEdit', false)
        }
      },
      popData: function(newVal, oldVal){
        let that = this
        // 重设基本选择位置
        console.log('weqweklscljwee')
        that.popData1 = newVal
        console.log(that.popData1)
        that.checkMark = that.popData1.headList[0].mark
        // 触发一下改变的验证位点
        that.scope1 = JSON.parse(JSON.stringify(that.scope))
      }
    },
    computed: {
      isCollapse() {
        return this.$store.getters.isOpened;
      },
      llen() {
        return this.popData1.headList.length
      }
    },
    filters: {
      formatDrugReactionType (value) {
        let n = ''
        for(let i in vm.drugReactionType) {
          if (value == vm.drugReactionType[i].value) {
            n = vm.drugReactionType[i].label
            break
          }
        }
        return n
      }
    }
  }
</script>
