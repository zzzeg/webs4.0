        }
    },
    created() {
      // 触发神经网络图(基因关系图)数据生成
      // this.gosetDefault()
      let that = this
      that.sWindow()
    },
    data () {
      vm = this
      return {
        // pageSize: 10
        popData1: {
          headList: [],
          datas: {}
        },
        cacheDataN: {},
        scope1: {},
        formData: {},
        formData1: {},
        drugReactionType: [{ //响应
            value: 0,
            label: '敏感 '
          },
          {
            value: 1,
            label: '耐药 '
          },
          {
