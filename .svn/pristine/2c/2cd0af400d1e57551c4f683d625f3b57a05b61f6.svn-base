        cydata: [],
        pushHeadData: [],
        secondData: {}
      }
    },
    mounted() {
      // this.gosetDefault()
    },
    methods: {
      goNewWindow(code) {
        // 跳转新页面
      },
      gosetDefault() {
        let that = this
        let s = that.cydata
        let cy =  that.$cytoscape({
          container: document.getElementById('cy'),
          layout: {
            name: 'cose',
            idealEdgeLength: 100,  // 莫名其妙的变大框
            nodeOverlap: 20,
            refresh: 20,
            fit: false, // 水平垂直居中
            padding: 0,
            randomize: false, // 暂时没发现用处
            componentSpacing: 100,
            nodeRepulsion: 400000, // 大小间距比例啥的
            edgeElasticity: 100, // 暂时没发现用处
            nestingFactor: 5, // 暂时没发现用处
            gravity: 500,
            numIter: 1000, // 这个一定要，随便设置，不给的话，各个圆会叠一块儿
            initialTemp: 200, // 暂时没发现用处
            coolingFactor: 0.9, // 散列点位置，值越低越贴近圆边
            minTemp: 1.0 // 没啥用
          },
          zoom: 1,
          zoomingEnabled: false,//定义是否可缩放
          style: [{
            "selector": "core",
            "style": {
              "selection-box-color": "#AAD8FF",
              "selection-box-border-color": "#8BB0D0",
              "selection-box-opacity": "0.5"
            }
          }, {
            "selector": "node",
            "style": {
              "width": "mapData(score, 0, 0.006769776522008331, 20, 60)",
              "height": "mapData(score, 0, 0.006769776522008331, 20, 60)",
              "content": "data(name)",
              "font-size": "12px",
              "text-valign": "center",
              "text-halign": "center",
              "background-color": "#555",
              "text-outline-color": "#555",
              "text-outline-width": "2px",
              "color": "#fff",
              "overlay-padding": "6px",
              "z-index": "10"
            }
          }, {
            "selector": "node[?attr]",
            "style": {
              "shape": "rectangle",
              "background-color": "#aaa",
              "text-outline-color": "#aaa",
              "width": "16px",
              "height": "16px",
              "font-size": "6px",
              "z-index": "1"
            }
          }, {
            "selector": "node[?query]",
            "style": {
