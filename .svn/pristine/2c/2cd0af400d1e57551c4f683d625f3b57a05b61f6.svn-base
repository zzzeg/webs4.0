            }
          }, {
            "selector": ".highlighted",
            "style": {
              "z-index": "999999"
            }
          }, {
            "selector": "node.highlighted",
            "style": {
              "border-width": "6px",
              "border-color": "#AAD8FF",
              "border-opacity": "0.5",
              "background-color": "#394855",
              "text-outline-color": "#394855"
            }
          }, {
            "selector": "edge.filtered",
            "style": {
              "opacity": "0"
            }
          }, {
            "selector": "edge[group=\"coexp\"]",
            "style": {
              "line-color": "#d0b7d5"
            }
          }, {
            "selector": "edge[group=\"coloc\"]",
            "style": {
              "line-color": "#a0b3dc"
            }
          }, {
            "selector": "edge[group=\"gi\"]",
            "style": {
              "line-color": "#90e190"
            }
          }, {
            "selector": "edge[group=\"path\"]",
            "style": {
              "line-color": "#9bd8de"
            }
          }, {
            "selector": "edge[group=\"pi\"]",
            "style": {
              "line-color": "#eaa2a2"
            }
          }, {
            "selector": "edge[group=\"predict\"]",
            "style": {
              "line-color": "#f6c384"
            }
          }, {
            "selector": "edge[group=\"spd\"]",
            "style": {
              "line-color": "#dad4a2"
            }
          }, {
            "selector": "edge[group=\"spd_attr\"]",
            "style": {
              "line-color": "#D0D0D0"
            }
          }, {
            "selector": "edge[group=\"reg\"]",
            "style": {
              "line-color": "#D0D0D0"
            }
          }, {
            "selector": "edge[group=\"reg_attr\"]",
            "style": {
              "line-color": "#D0D0D0"
            }
          }, {
            "selector": "edge[group=\"user\"]",
            "style": {
              "line-color": "#f0ec86"
            }
          }],
          elements: s
        })
        return false
      },
      drawCloud(){
        let that = this
        // alert('eeeeee')
        let charObj = document.getElementById('wordCloud')
        charObj.style.width = '500px';
        let wordCloud = this.$echarts.init(charObj);
        // wordCloud.resize()
        // wordCloud.style.width = 500 + 'px';
        function createRandomItemStyle() {
            return {
                normal: {
                    color: 'rgb(' + [
                        Math.round(Math.random() * 160),
                        Math.round(Math.random() * 160),
                        Math.round(Math.random() * 160)
                    ].join(',') + ')'
                }
            }
        }
        let text = []
        for (var qq = 0; qq < that.termData.length; qq++) {
          text.push(that.termData[qq])
        }
        let data = []
        for(var i = 0; i < text.length; i++){
            let obj = {};
            obj.name = text[i];
            obj.value = Math.random()*10000;
            data.push(obj)
        }
        console.log('data is:')
        console.log(data)
        let option = {
            title: {
                text: null,
                link: ''
            },
            tooltip: {
                show: false, // 悬停提示
            },
            series: [{
                name: 'Force tree',
                type: 'wordCloud',
                sizeRange: [18, 20],//画布范围，如果设置太大会出现少词（溢出屏幕）
                rotationRange: [0, 15],//数据翻转范围
                //shape: 'circle',
                textPadding: 0,
                autoSize: {
                    enable: true,
                    minSize: 20
                },
                textStyle: {
                    normal: {
                        color: function() {
                            return 'rgb(' + [
                                Math.round(Math.random() * 160),
                                Math.round(Math.random() * 160),
                                Math.round(Math.random() * 160)
                            ].join(',') + ')';
                        }
                    },
                    emphasis: {
                        shadowBlur: 10,
                        shadowColor: '#333'
                    }
                },
                data: data
            }]
        }
        wordCloud.setOption(option)
      },
      handleCurrentChange (val) {
        this.$emit('handleCurrentChange', val)
      },
      handleSizeChange (val) {
        this.$emit('handleSizeChange', val)
      },
      sWindow(type, keyVal) {
        // 查询的类型   基因、疾病、药物    keyVal 是查询参数
        let that = this
        that.$emit('sWindow')
        that.$nextTick(function () {
          that.gosetDefault()
          that.drawCloud()
        })
      },
      changeTab(item, i) {
        // 改变选中的选项
        let that = this
        if (i == 0) {
          let n = JSON.parse(JSON.stringify(that.popData1.headList))
          for (let ec in n) {
            n[ec].checkNum = false
          }
          n[0].checkNum = true

          that.$emit('sWindow')
          that.$nextTick(function () {
            that.gosetDefault()
            that.drawCloud()
          })
          setTimeout(function(){
            that.popData1.headList = JSON.parse(JSON.stringify(n))
          }, 500)
        }
        that.$emit('changeTab', item, i)
        // 算一哈不能关闭的菜单有几个
        let qq = 0
        for (let s in that.popData1.headList) {
          if (that.popData1.headList[s].canClose == false) {
            qq = qq + 1
          }
        }
        // 总共的菜单减去不能关闭的菜单  剩下就是能关闭的菜单，拿到索引赋值给cacheDataN
        let num = i - qq
        if (i >= qq) {
          that.cacheDataN = that.pushHeadData[num]
        }
        // console.log('xxxxxxxxxxxxxxxxxxxxxxxxxxxxx')
        // console.log(num)
        // console.log(that.pushHeadData)
        // console.log(that.cacheDataN)
        // console.log('item is :' + item)
        // console.log('that.checkMark is :' + that.checkMark)
      },
      pushHead(type, scope) {
        let that = this
        that.$emit('pushHead', type, scope)
        if (type == 'disease') {
          // 调用疾病查询接口
          that.selectCacheData(type, scope.diseaseEnName)
        } else if (type == 'drug') {
          // 调用药物查询接口
          that.selectCacheData(type, scope.drugEnName)
        }
      },
      selectCacheData(type, searchKey) {
        let that = this
        let url = type == 'disease' ? 'report/getDiseaseDetails' : 'report/getDrugDetails'
        let param = {}
        if (type == 'disease') {
          param = {
            diseaseEnName: searchKey
          }
        } else if (type == 'drug') {
          param = {
            drugEnName: searchKey
          }
        }
        axios.get(URL.api_name + url,  {
          params: param
        }).then(function (res) {
          if (res.data.code === '100') {
            // that.cacheDataN = res.data.data
            that.pushHeadData.push(res.data.data)
            let n = that.pushHeadData.length - 1
            that.cacheDataN = that.pushHeadData[n]
            console.log('xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx')
            console.log(that.cacheDataN)
          } else {
            that.$message({
              type: 'error',
              message: res.data.message,
              duration: 1000
            })
          }
        })
       },
      cmmk(i) {
        let that = this
        that.$emit('cmmk', i)
      },
      closePanel(i) {
        let that = this
        that.$emit('closePanel', i)
      },
      blockSearch(formInlines) { // 查询
        let that = this
        that.$emit('blockSearch', formInlines)
       },
      changeValidata(value) {
        // 更改标记的提示
        let that = this
        that.scope1.hasValidata = value == '0' ? '1' : '0'
        let url = 'report/variant/modifyReportVariantValidate'
        axios.post(URL.api_name + url, {
          code: that.scope1.code,
          hasValidata: that.scope1.hasValidata
        }).then(function(res) {
          if(res.data.code == '100') {
            that.$message({
              type: 'success',
              message: res.data.message,
              duration: 1000
            })
            that.blockSearch(that.formInline)
          } else {
            // 失败,重新查询
            that.$message({
              type: 'error',
              message: res.data.message,
              duration: 1000
            })
          }
        }).catch(function(res){
          // 接口报错了
        })
      }
    },
    components: {
      Pager: Pager
    },
    watch: {
      formDatas: function(newVal,oldVal){
        let that = this
        this.formData = newVal
        this.formData1 = JSON.parse(JSON.stringify(this.formData))
        for (let i in that.formData1.list) {
          // that.formData1.list[i]['isEdit'] = false
          that.$set(that.formData1.list[i], 'isEdit', false)
        }
      },
      popData: {
        handler: function(newVal, oldVal){
          let that = this
          // 重设基本选择位置
          that.popData1 = newVal
          // 触发一下改变的验证位点
          that.scope1 = JSON.parse(JSON.stringify(that.scope))
          that.termData = []
          // 右侧图片内容
          for (let i in that.popData1.datas.goTerms) {
            that.termData.push(that.popData1.datas.goTerms[i].term)
          }
          // 左侧图片内容
          that.cydata = (that.popData1.datas.molecularNetworkImg && that.popData1.datas.molecularNetworkImg != '') ? JSON.parse(that.popData1.datas.molecularNetworkImg) : []

          if (that.checkMark == 'base') {
            that.gosetDefault()
            that.drawCloud()
          }

        },
        deep: true,
        // immediate: true
      },
      checkMark: {
        handler: function(newVal, oldVal){
          let that = this
          if (that.checkMark == 'drugF') {
            console.log('xxxxxxxxxxxxxxxxxxxxx')
            console.log(that.popData1)
          }
        },
        deep: true,
        // immediate: true
      }
    },
    computed: {
      isCollapse() {
        return this.$store.getters.isOpened;
      },
      llen() {
        return this.popData1.headList.length
      }
    },
    filters: {
      formatDrugReactionType (value) {
        let n = ''
        for(let i in vm.drugReactionType) {
          if (value == vm.drugReactionType[i].value) {
            n = vm.drugReactionType[i].label
            break
          }
        }
        return n
      }
    }
  }
